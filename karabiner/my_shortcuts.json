{
    "title": "My Shortcuts",
    "rules": [
        {
            "description": "Disable Cmd+M (minimize) on all applications",
            "manipulators": [
                {
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": [
                                "left_command"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "m",
                            "modifiers": [
                                "left_command",
                                "left_shift",
                                "left_control"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}